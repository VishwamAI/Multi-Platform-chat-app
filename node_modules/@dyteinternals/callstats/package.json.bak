{
  "name": "@dyte-in/dyte-callstats-toolkit",
  "version": "2.3.4",
  "description": "An internal toolkit to obtain call statistic for Dyte meetings.",
  "author": "dyte-in",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dyte-in/callstats-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/dyte-in/callstats-toolkit/issues"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -W",
    "build:lib": "rollup -c",
    "test": "jest",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "deploy": "gh-pages -d example/build",
    "prepare": "is-ci || husky install",
    "lint": "eslint . --ext .ts --ext .js --ext .tsx --ext .jsx",
    "lint:fix": "eslint . --ext .ts --ext .js  --ext .tsx --ext .jsx --fix",
    "prepublishOnly": "cp package.json package.json.bak && node prepublish.js",
    "postpublish": "mv package.json.bak package.json"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "peerDependencies": {
    "react": "*",
    "@dytesdk/mediasoup-client": "*"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "querystring-es3": "^0.2.1"
  },
  "devDependencies": {
    "@aws-sdk/client-sns": "^3.27.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/aws4": "^1.5.2",
    "@types/classnames": "^2.2.11",
    "@types/estree": "^0.0.47",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/ws": "^7.4.5",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-airbnb": "*",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^7.0.0",
    "is-ci": "^2.0.0",
    "jest": "^27.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.3",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.3"
  },
  "optionalDependencies": {
    "utf-8-validate": "^5.0.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
